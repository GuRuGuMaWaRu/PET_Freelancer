  "description": "log freelance jobs",
  "main": "index.js",
  "scripts": {
    "start": "node app.js",
    "server": "concurrently \"tsc -w\" \"nodemon ./dist/app.js\"",
    "client": "npm start --prefix client_ts_redux",
    "client:install": "npm install --prefix client_ts_redux",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
